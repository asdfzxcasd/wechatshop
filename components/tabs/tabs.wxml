<view class="tabs">
  <view class="tabs_title">
    <view class="title_item {{item.isActive?'active':''}}"
     wx:for="{{tabs}}" 
     wx:key="id"
     bindtap="handleitem"
     data-index="{{index}}">
     {{item.value}}
     </view>
  </view>
  <view class="tabs_centent">
    <slot></slot>
  
  </view>

</view>
